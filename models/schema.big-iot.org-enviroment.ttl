  rdfs:label "Carbon Dioxide (CO2)"^^xsd:string ;
.
environment:GHG
  rdf:type rdfs:Class ;
  rdfs:subClassOf environment:AirPollutionIndicator;
  rdfs:comment "Chemical compound responsible for greenhouse effect in gas form (greenhous gas)."^^xsd:string ;
