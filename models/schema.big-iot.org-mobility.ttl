mobility:trafficDetectorLocation a rdf:Property ;
  rdfs:subPropertyOf common:geoJSONLocation;
  rdfs:label "traffic detector location midpoint"^^xsd:string ;
  rdfs:comment "The coordinates of a traffic detector location: the GeoJSON geometry object of type Point with an array of coordinates will be returned."^^xsd:string ;
  schema:domainIncludes mobility:TrafficDetector ;
  schema:rangeIncludes common:GeoJSONGeometryObject .

  mobility:jamFactorId rdf:type rdf:Property ;
  rdfs:label	"jam factor identifier" ;
  rdfs:comment	"an identifier of a jam factor " ;
  rdfs:subPropertyOf schema:identifier ;
  schema:domainIncludes mobility:TrafficData ;
  schema:domainIncludes mobility:TrafficDetector ;
  schema:rangeIncludes schema:Integer .  

mobility:alertCDirection rdf:type rdf:Property ;
   rdfs:label	"alert C direction" ;
   rdfs:comment	"The direction of traffic flow along the road to which the information relates." ;
   schema:domainIncludes mobility:TrafficData ;
   schema:rangeIncludes schema:Text ;
   rdfs:subPropertyOf common:numericValue ;
   dc:source <http://vocab.datex.org/terms#AlertCDirection> .	
	
mobility:lengthAffected rdf:type rdf:Property ;
   rdfs:label	"length affected" ;
   rdfs:comment	"This indicates the length of road measured in metres affected by the associated traffic element." ;
   schema:domainIncludes mobility:TrafficData ;
   schema:rangeIncludes schema:Float ;
   rdfs:subPropertyOf common:numericValue ;
   dc:source <http://vocab.datex.org/terms#lengthAffected> .		

mobility:freeFlowSpeed rdf:type rdf:Property ;
   rdfs:label	"free flow speed" ;
   rdfs:comment	"The free flow speed expected under ideal conditions, corresponding to the freeFlowTravelTime." ;
   schema:domainIncludes mobility:TrafficData ;
   schema:rangeIncludes schema:Float ;
   rdfs:subPropertyOf common:numericValue ;
   dc:source <http://vocab.datex.org/terms#freeFlowSpeed> .	

mobility:maximumSpeedLimit rdf:type rdf:Property ;
  rdfs:label	"maximum speed limit" ;
  rdfs:comment	"Maximum speed limit." ;
  schema:domainIncludes mobility:TrafficData ;
  schema:rangeIncludes schema:Float ;
  rdfs:subPropertyOf common:numericValue ;
  dc:source <http://vocab.datex.org/terms#maximumSpeedLimit> .

mobility:pointTMCLocationCode rdf:type rdf:Property ;
  rdfs:label	"point TMC location code" ;
  rdfs:comment	"The geographic information used for TMC (Traffic Messaging Channel) is distributed in the form of location code lists. Their format is defined in the standard ISO 14819-3." ;
  rdfs:subPropertyOf common:numericValue ;
  schema:domainIncludes mobility:TrafficData ;
  schema:rangeIncludes schema:Number .

mobility:speedConfidence rdf:type rdf:Property ;
  rdfs:label	"speed confidence" ;
  rdfs:comment	"An indication how the speed was determined.  1.0 - 100%. -1 means road closed." ;
  rdfs:subPropertyOf common:numericValue ;
  schema:domainIncludes mobility:TrafficData ;
  schema:rangeIncludes schema:Number .

mobility:jamFactor rdf:type rdf:Property ;
  rdfs:label	"jam factor" ;
  rdfs:comment	"Describes characteristics of traffic flow. When the extension of type String is expected, potential values such as erraticFlow, smoothFlow, stopAndGo, trafficBlocked are to be enumerated " ;
  schema:domainIncludes mobility:TrafficData ;
  schema:domainIncludes mobility:TrafficDetector ;
  schema:rangeIncludes mobility:TrafficFlowCharacteristics ;
  schema:rangeIncludes schema:Text .

mobility:TrafficFlowCharacteristics rdf:type rdfs:Class;
  rdfs:label	"Traffic Flow Characteristics" ;
  rdfs:comment	"A collection of terms for describing the characteristics of traffic flow." ;
  rdfs:subClassOf	schema:Enumeration ;
  dc:source <http://vocab.datex.org/terms#TrafficFlowCharacteristicsEnum> .

mobility:erraticFlow a mobility:TrafficFlowCharacteristics ;
  rdfs:comment "Traffic flow is of an irregular nature, subject to sudden changes in rates." ;
  dc:source <http://vocab.datex.org/terms#erraticFlow> ;
  rdfs:label "erratic flow" .
	
mobility:smoothFlow a mobility:TrafficFlowCharacteristics ;
  rdfs:comment "Traffic flow is smooth." ;
  dc:source <http://vocab.datex.org/terms#smoothFlow> ;
  rdfs:label "smooth flow" .	
	
mobility:stopAndGo a mobility:TrafficFlowCharacteristics ;
  rdfs:comment "Traffic flow is of a stop and go nature with queues forming and ending continuously on the specified section of road." ;
  dc:source <http://vocab.datex.org/terms#stopAndGo> ;
  rdfs:label "stop and go" .
	
mobility:trafficBlocked a mobility:TrafficFlowCharacteristics ;
  rdfs:comment "Traffic is blocked at the specified location and in the specified direction due to an unplanned event." ;
  dc:source <http://vocab.datex.org/terms#trafficBlocked> ;
  rdfs:label "traffic blocked" .


