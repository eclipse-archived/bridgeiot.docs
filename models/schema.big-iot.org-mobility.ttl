mobility:highest a mobility:SeverityOfTrafficObstacle ;
    rdfs:label "highest" ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:comment "Perceived by supplier as being of the highest level." .
	
mobility:high a mobility:SeverityOfTrafficObstacle ;
    rdfs:label "high" ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:comment "Perceived by supplier as being of a high level." .	
	
mobility:medium a mobility:SeverityOfTrafficObstacle ;
    rdfs:label "medium" ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:comment "Perceived by supplier as being of a medium level." .
	
mobility:low a mobility:SeverityOfTrafficObstacle ;
    rdfs:label "low" ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:comment "Perceived by supplier as being of a low level." .		
	
mobility:lowest a mobility:SeverityOfTrafficObstacle ;
    rdfs:label "lowest" ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:comment "Perceived by supplier as being of the lowest discernible level." .
	
mobility:none a mobility:SeverityOfTrafficObstacle ;
    rdfs:label "none" ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:comment "Perceived by supplier as having a severity rating of none." .	
	
mobility:unknown a mobility:SeverityOfTrafficObstacle ;
    rdfs:label "unknown" ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:comment "Perceived by supplier as being of an unknown level." .	
	
mobility:trafficObstacleSeverity a rdf:Property ;
    rdfs:label "traffic obstacle severity" ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    schema:domainIncludes mobility:TrafficObstacle ;
    schema:rangeIncludes mobility:SeverityOfTrafficObstacle ;
    rdfs:comment "The severity level of a traffic obstacle: highest. high, medium, low, lowest, none, unknown." .	
	
mobility:trafficObstacleStatus a rdf:Property ;
    rdfs:label "traffic obstacle status" ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    schema:domainIncludes mobility:TrafficObstacle ;
    schema:rangeIncludes mobility:TrafficObstacleStatus ;
    rdfs:comment "The status of a traffic obstacle report: active / inactive." .		

mobility:TrafficObstacleStatus a rdfs:Class ;
    rdfs:label "Traffic Obstacle Status" ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:comment "Status of a traffic obstacle." ;
    rdfs:subClassOf schema:Enumeration .
	
mobility:active a mobility:TrafficObstacleStatus ;
    rdfs:label "active" ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:comment "active traffic obstacle." .	
	
mobility:inactive a mobility:TrafficObstacleStatus ;
    rdfs:label "inactive" ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:comment "Inactive traffic obstacle." .		
  
mobility:trafficObstacleVerified
  rdf:type rdf:Property ;
  schema:domainIncludes mobility:TrafficObstacle ;
  schema:rangeIncludes schema:Boolean ;
