mobility:MobilityFeature a rdfs:Class ;
  rdfs:label "Mobility"^^xsd:string ;
  skos:prefLabel "Mobility"^^xsd:string ;
  rdfs:comment "Any mobility-related feature of interest (road, bus, parking site, etc.)"^^xsd:string ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf sosa:FeatureOfInterest .

mobility:Bus a rdfs:Class ;
  rdfs:label "Bus"^^xsd:string ;
  rdfs:comment "Any bus-related feature of interest."^^xsd:string ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf schema:BusOrCoach ;
  rdfs:subClassOf mobility:Transportation .
  
mobility:BusStop a rdfs:Class ;
  rdfs:label "Bus Stop"^^xsd:string ;
  rdfs:comment "A bus stop."^^xsd:string ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf schema:BusStop .

mobility:liveBusPosition a rdf:Property ;
  rdfs:label "live bus position"^^xsd:string ;
  rdfs:comment "Live position of a bus or a coach."^^xsd:string ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  schema:domainIncludes mobility:Bus ;
  schema:rangeIncludes mobility:LiveBusPosition .

mobility:LiveBusPosition a rdfs:Class ;
  rdfs:label "Live Bus Position"^^xsd:string ;
  rdfs:comment "Live Position of a Bus class"^^xsd:string ;
  schema:isPartOf <http://schema.big-iot.org/mobility> .

mobility:Parking a rdfs:Class ;
  rdfs:label "Parking"^^xsd:string ;
  rdfs:comment "E.g. parking site or parking space."^^xsd:string ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf mobility:MobilityFeature .

mobility:ParkingSite a rdfs:Class ;
  rdfs:label "Parking Site"^^xsd:string ;
  rdfs:comment "A parking site."^^xsd:string ;
  dc:source <http://vocab.datex.org/terms#ParkingSite> ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf schema:ParkingFacility ;
  rdfs:subClassOf mobility:Parking .

mobility:ParkingSpace a rdfs:Class ;
  rdfs:label "Parking Space"^^xsd:string ;
  rdfs:comment "A single parking space. It is possible to define the same parking space more than once with different properties, e.g. when there is a different parking assignment for different times."^^xsd:string ;
  dc:source <http://vocab.datex.org/terms#ParkingSpace> ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf schema:ParkingFacility ;
  rdfs:subClassOf mobility:Parking .

mobility:ParkingSpaceStatus a rdfs:Class ;
  rdfs:label "Parking Space Status"^^xsd:string ;
  rdfs:comment "Current status of a parking space."^^xsd:string ;
  dc:source <http://vocab.datex.org/terms#ParkingSpaceStatus> ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf common:Measurement ;
  rdfs:subClassOf schema:Enumeration .

mobility:parkingSpaceLocationMidpoint a rdf:Property ;
  rdfs:subPropertyOf common:geoJSONLocation;
  rdfs:label "parking space location midpoint"^^xsd:string ;
  rdfs:comment "The midpoint of a parking space location: the GeoJSON geometry object of type Point with an array of coordinates will be returned."^^xsd:string ;
  schema:domainIncludes mobility:ParkingSpace ;
  schema:rangeIncludes common:GeoJSONGeometryObject .
  
mobility:parkingSpaceLocationBoundingBox a rdf:Property ;
  rdfs:subPropertyOf common:geoJSONLocation;
  rdfs:label "parking space location bounding box"^^xsd:string ;
  rdfs:comment "The bounding box of a parking space location: the GeoJSON geometry object of type Polygon with an array of coordinates will be returned."^^xsd:string ;
  schema:domainIncludes mobility:ParkingSpace ;
  schema:rangeIncludes common:GeoJSONGeometryObject .

mobility:PeopleDensityEstimation a rdfs:Class ;
  rdfs:label "People Density Estimation"^^xsd:string ;
  rdfs:comment "Estimation of people density."^^xsd:string ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf common:Concentration ;
  rdfs:subClassOf mobility:MobilityFeature .

mobility:PeopleDensityInArea a rdfs:Class ;
  rdfs:label "People Density In Area"^^xsd:string ;
  rdfs:comment "People density in a given area."^^xsd:string ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf mobility:PeopleDensityEstimation .

mobility:PeopleMobilityWithinArea a rdfs:Class ;
  rdfs:label "People Mobility Within Area"^^xsd:string ;
  rdfs:comment "People mobility within a given area."^^xsd:string ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf mobility:PeopleDensityEstimation .

mobility:PeopleDensityOnBus a rdfs:Class ;
  rdfs:label "People Density On Bus"^^xsd:string ;
  rdfs:comment "People density on a bus"^^xsd:string ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf mobility:PeopleDensityEstimation .

mobility:tpegAreaLocation a rdf:Property ;
  rdfs:label "TPEG area location"^^xsd:string ;
  rdfs:comment "A geographic or geometric area defined by a TPEG-Loc structure which may include height information for additional geospatial discrimination."^^xsd:string ;
  dc:source <http://vocab.datex.org/terms#tpegAreaLocation> ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  schema:domainIncludes mobility:MobilityFeature ;
  schema:rangeIncludes mobility:TpegAreaLocation .

mobility:tpegLinearLocation a rdf:Property ;
  rdfs:label "TPEG linear location"^^xsd:string ;
  rdfs:comment "A linear section along a single road defined between two points on the same road by a TPEG-Loc structure."^^xsd:string ;
  dc:source <http://vocab.datex.org/terms#tpegLinearLocation> ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  schema:domainIncludes mobility:MobilityFeature ;
  schema:rangeIncludes mobility:TpegLinearLocation .

mobility:tpegGeometricArea a rdf:Property ;
  rdfs:label "TPEG geometric area"^^xsd:string ;
  rdfs:comment "A geometric area defined by a centre point and a radius."^^xsd:string ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  schema:domainIncludes mobility:MobilityFeature ;
  schema:rangeIncludes mobility:TpegGeometricArea .

mobility:TpegAreaLocation a rdfs:Class ;
  rdfs:label "TPEG Area Location"^^xsd:string ;
  rdfs:comment "A geographic or geometric area defined by a TPEG-Loc structure which may include height information for additional geospatial discrimination."^^xsd:string ;
  dc:source <http://vocab.datex.org/terms#TpegAreaLocation> ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf schema:GeospatialGeometry .

mobility:TpegGeometricArea a rdfs:Class ;
  rdfs:label "TPEG Geometric Area"^^xsd:string ;
  rdfs:comment "A geometric area defined by a centre point and a radius."^^xsd:string ;
  dc:source <http://vocab.datex.org/terms#TpegGeometricArea> ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf mobility:TpegAreaLocation .

mobility:TpegLinearLocation a rdfs:Class ;
  rdfs:label "TPEG Linear Location"^^xsd:string ;
  rdfs:comment "A linear section along a single road defined between two points on the same road by a TPEG-Loc structure."^^xsd:string ;
  dc:source <http://vocab.datex.org/terms#TpegLinearLocation> ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf schema:GeospatialGeometry .

mobility:Traffic a rdfs:Class ;
  rdfs:label "Traffic"^^xsd:string ;
  rdfs:comment "Any traffic-related feature of interest."^^xsd:string ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf mobility:MobilityFeature .
    
mobility:hasTrafficData a rdf:Property ;
  rdfs:label "has traffic data"^^xsd:string ;
  rdfs:comment "Data that characterizes traffic."^^xsd:string ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  schema:domainIncludes mobility:Traffic ;
  schema:rangeIncludes mobility:TrafficData .

mobility:TrafficData a rdfs:Class ;
  rdfs:label "Traffic Data"^^xsd:string ;
  rdfs:comment "Measured or derived values relating to traffic or individual vehicle movements on a specific section or at a specific point on the road network."^^xsd:string ;
  dc:source <http://vocab.datex.org/terms#TrafficData> ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf mobility:Traffic .

mobility:Transportation a rdfs:Class ;
  rdfs:label "Transportation"^^xsd:string ;
  rdfs:comment "A transportation category class."^^xsd:string ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf mobility:MobilityFeature .

mobility:TravelTime a rdfs:Class ;
  rdfs:label "Travel Time"^^xsd:string ;
  rdfs:comment "Derived/computed travel time information relating to a linear section of the road network; forecast = true means a forecast for a vehicle at the start of the specified location, forecast = false means calculation/measurement at the end."^^xsd:string ;
  dc:source <http://vocab.datex.org/terms#TravelTimeData> ;
  schema:isPartOf <http://schema.big-iot.org/mobility> ;
  rdfs:subClassOf mobility:MobilityFeature .

mobility:atBusLocation a rdf:Property ;
