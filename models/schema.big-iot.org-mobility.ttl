mobility:abnormalLoad a mobility:TrafficObstacleKind ;
    rdfs:label "abnormal load" ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:comment "Vehicle(s) carrying exceptional load(s) which may cause traffic disruption." .	
	
mobility:other a mobility:TrafficObstacleKind ;
    rdfs:label "other" ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
