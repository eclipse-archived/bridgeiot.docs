mobility:environmentalObstruction a mobility:TrafficObstacleKind ;
    rdfs:label "environmental obstruction" ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:comment "An obstruction on the road resulting from an environmental cause." .	

mobility:trafficObstacleKind a rdf:Property ;
