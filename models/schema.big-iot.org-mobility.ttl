# Car tracking and on-board diagnostics modeling

mobility:CarDiagnostics a rdfs:Class ;
    rdfs:label "Car Diagnostics"^^xsd:string ;
    rdfs:comment "Car parameters related to on-board diagnostics: data gathered, for instance, by the device plugged into the OBD II port."^^xsd:string ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:seeAlso schema:Car ;
    rdfs:subClassOf mobility:Transportation .
  	
mobility:LocationTracking a rdfs:Class ;
    rdfs:label "Location Tracking"^^xsd:string ;
    rdfs:comment "Car parameters related to location tracking: data collected by location tracking devices (either installed in the vehicle or received via receiver-equipped mobile devices) using GPS, GLONASS or other technology."^^xsd:string ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:subClassOf mobility:Transportation .

mobility:measuredSpeed a rdf:Property ;
    rdfs:label "measured speed"^^xsd:string ;
    rdfs:comment "The vehicle speed, measured in kilometer per hour; different from schema.org/Car.speed property which indicates speed range of a vehicle."^^xsd:string ;
    dc:source <http://schema.org/speed> ;
    dc:source <http://vocab.datex.org/terms#individualVehicleSpeed> ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:subPropertyOf mobility:carParameter;
    schema:domainIncludes mobility:CarDiagnostics ;
    schema:rangeIncludes schema:Number;
    rdfs:seeAlso mobility:gpsSpeed .
    
mobility:emissionsCO2 a rdf:Property ;
    rdfs:label "emissions CO2"^^xsd:string ;
    rdfs:comment "The carbon dioxide (CO2) emissions, measured in kilogram per hour; the identically titled schema.org/Car property declares measurements in g/km."^^xsd:string ;
    dc:source <http://schema.org/emissionsCO2> ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:subPropertyOf mobility:carParameter;
    schema:domainIncludes mobility:CarDiagnostics ;
    schema:rangeIncludes schema:Number;
    rdfs:seeAlso environment:CO2 .

mobility:fuelConsumption a rdf:Property ;
    rdfs:label "fuel consumption"^^xsd:string ;
    rdfs:comment "The amount of the consumed fuel, measured in liter per hour. The corresponding schema.org property declares example measurements in L/100 km."^^xsd:string ;
    dc:source <http://schema.org/fuelConsumption> ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:subPropertyOf mobility:carParameter;
    schema:domainIncludes mobility:CarDiagnostics ;
    schema:rangeIncludes schema:Number .

mobility:massAirFlow a rdf:Property ;
    rdfs:label "mass air flow (MAF)"^^xsd:string ;
    rdfs:comment "The mass flow rate of air entering a fuel-injected internal combustion engine, measured in gram per second; used by the the engine control unit to balance and deliver the correct fuel mass to the engine.^^xsd:string" ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:subPropertyOf mobility:carParameter;
    schema:domainIncludes mobility:CarDiagnostics ;
    schema:rangeIncludes schema:Number .
    
mobility:calculatedMassAirFlow a rdf:Property ;
    rdfs:label "calculated mass air flow (MAF)"^^xsd:string ;
    rdfs:comment "The mass air flow calculated using various parameters (intake air temperature, intake manifold absolute pressure, engine revolutions per minute and other) when the OBD adapter delivers no result for MAF; measured in gram per second."^^xsd:string ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:subPropertyOf mobility:carParameter;
    schema:domainIncludes mobility:CarDiagnostics ;
    schema:rangeIncludes schema:Number .

mobility:intakeAirTemperature a rdf:Property ;
    rdfs:label "intake air temperature"^^xsd:string ;
    rdfs:comment "The temperature of the air entering the engine, measured in degrees Celsius; used by the engine control unit to assist in determining the correct engine fuel requirement to suit the operating air temperature."^^xsd:string ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:subPropertyOf mobility:carParameter;
    schema:domainIncludes mobility:CarDiagnostics ;
    schema:rangeIncludes schema:Number .
   
mobility:intakeManifoldAbsolutePressure a rdf:Property ;
    rdfs:label "intake manifold absolute pressure (MAP)"^^xsd:string ;
    rdfs:comment "The air pressure in the intake manifold, measured in kilopascal (kPa) absolute; used to calculate air density and determine the engine's air mass flow rate."^^xsd:string ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:subPropertyOf mobility:carParameter;
    schema:domainIncludes mobility:CarDiagnostics ;
    schema:rangeIncludes schema:Number .

mobility:engineRevolutionsPerMinute a rdf:Property ;
    rdfs:label "engine revolutions per minute (RPM)"^^xsd:string ;
    rdfs:comment "The engine's crankshaft revolutions per minute, the number of rotations around a fixed axis in one minute."^^xsd:string ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:subPropertyOf mobility:carParameter;
    schema:domainIncludes mobility:CarDiagnostics ;
    schema:rangeIncludes schema:Number .

mobility:calculatedEngineLoad a rdf:Property ;
    rdfs:label "calculated engine load"^^xsd:string ;
    rdfs:comment "Calculated engine load indicates a percentage of peak available torque."^^xsd:string ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:subPropertyOf mobility:carParameter;
    schema:domainIncludes mobility:CarDiagnostics ;
    schema:rangeIncludes schema:Number .

mobility:fuelSystemStatusCode a rdf:Property ;
    rdfs:label "fuel system status code"^^xsd:string ;
    rdfs:comment "The code returned as a response to PID-supported command 01 03 indicating the fuel system loop status (e.g. 1: open loop due to insufficient engine temperature)."^^xsd:string ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:subPropertyOf mobility:carParameter;
    schema:domainIncludes mobility:CarDiagnostics ;
    schema:rangeIncludes schema:Number .

mobility:longTermFuelTrim1 a rdf:Property ;
    rdfs:label "long term fuel trim-bank1"^^xsd:string ;
    rdfs:comment "Averaged over time adjustment to the fuel injector pulse width (and thus amount of fuel); measured as percentage."^^xsd:string ;
    schema:isPartOf <http://schema.big-iot.org/mobility> ;
    rdfs:subPropertyOf mobility:carParameter;
    schema:domainIncludes mobility:CarDiagnostics ;
    schema:rangeIncludes schema:Number .
    
mobility:shortTermFuelTrim1 a rdf:Property ;
    rdfs:label "short term fuel trim-Bank1"^^xsd:string ;
